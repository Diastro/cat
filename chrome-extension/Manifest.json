{
  "name": "Chrome activity tracker",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Chrome activity tracker",
  "permissions": [
    "tabs",
    "notifications",
    "http://*/*"
  ],
    "background": {
    "scripts": ["jquery-2.1.3.min.js", "background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "images/offline.png"
    },
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "run_at": "document_end",
    "js": ["jquery-2.1.3.min.js", "content.js"]
  }
  ]
}